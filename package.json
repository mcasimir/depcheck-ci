{
  "name": "depcheck-ci",
  "version": "1.1.0",
  "engines": {
    "node": ">= 8"
  },
  "description": "Depcheck util that plays nice with ci",
  "author": "mcasimir",
  "main": "src/Depcheck.js",
  "license": "MIT",
  "scripts": {
    "release": "release-flow",
    "test:lint": "eslint . --quiet",
    "test:ci": "npm run test:lint && npm run depcheck",
    "depcheck": "node ./lib/cli.js"
  },
  "bin": {
    "depcheck-ci": "./lib/cli.js"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "depcheck": "0.7.2",
    "yargs": "5.0.0"
  },
  "devDependencies": {
    "eslint": "5.14.1",
    "eslint-config-google": "0.11.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-mocha": "5.2.1",
    "eslint-plugin-no-async-without-await": "1.2.0",
    "eslint-plugin-node": "8.0.1",
    "release-flow": "1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mcasimir/depcheck-ci.git"
  }
}
